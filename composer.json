{
  "name": "bnbwebexpertise/laravel-bootstrap-form",
  "description": "Laravel 5 form wrappers for Bootstrap 3. Based on Dwight Watson version and tuned for personal requirements.",
  "keywords": [
    "laravel",
    "form",
    "bootstrap"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Dwight Watson",
      "email": "dwightwatson@me.com"
    },
    {
      "name": "Jérémy Gaulin",
      "email": "jeremy@bnb.re"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/config": ">=5.0",
    "illuminate/session": ">=5.0",
    "illuminate/support": ">=5.0",
    "laravelcollective/html": ">=5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "psr-4": {
      "Bnb\\BootstrapForm\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev",
      "1-dev": "1.x-dev"
    },
    "laravel": {
      "providers": [
        "Bnb\\BootstrapForm\\BootstrapFormServiceProvider"
      ],
      "aliases": {
        "BootForm": "Bnb\\BootstrapForm\\Facades\\BootstrapForm"
      }
    }
  },
  "minimum-stability": "dev"
}
